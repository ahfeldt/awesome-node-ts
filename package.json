{
  "dependencies": {
    "dotenv": "^17.2.1",
    "fastify": "^5.5.0",
    "pino": "^9.9.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vitest": "^3.2.4"
  },
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec \"tsx src/server.ts\"",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier -w .",
    "test": "vitest run",
    "test:watch": "vitest"
  }
}